#ifndef PWM_TIMER0_PRIVATE_H_
#define PWM_TIMER0_PRIVATE_H_
#include "std_types.h"
/*******************************************************************************
 *                  Registers type structure declaration                       *
 *******************************************************************************/

/* TIMER0 TCNCTO  Register type structure declarations */
typedef union
{
	uint8 Byte;
	struct
	{
		uint8 BIT:1;
		uint8 BIT:1;
		uint8 BIT:1;
		uint8 BIT:1;
		uint8 BIT:1;
		uint8 BIT:1;
		uint8 BIT:1;
		uint8 BIT:1;
	}Bits;
}TIMER0_TCNT0_Reg_Type;


#endif /* PWM_TIMER0_PRIVATE_H_ */
